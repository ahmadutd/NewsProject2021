@page
@model AllNewsCms.Web.Pages.AdminsControl.EditModel
@{
    ViewData["Title"] = "Edit";
}
    <br />
<div class="row">
    <div class="col-md-6 m-auto">
        <div class="alert alert-success d-flex align-items-center">
            <h4 class="fas fa-user mr-2"></h4>
            <h4>Edit User For</h4>
            <h4 class="ml-2 text-warning">@Model.UserList.UserName</h4>
        </div>
        <hr />
        <form method="post">

            <input type="hidden" asp-for="@Model.UserList.Id" />
            <div class="form-group">
                <label asp-for="UserList.Email"></label>
                <input asp-for="UserList.Email" class="form-control" />

            </div>
            <div class="form-group">
                <label asp-for="UserList.UserName"></label>
                <input asp-for="UserList.UserName" class="form-control" />

            </div>

            <br />
            <button type="submit" class="btn btn-outline-success m-auto">Save</button>
        </form>
    </div>


    


</div>

